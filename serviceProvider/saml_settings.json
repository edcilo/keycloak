{
    "strict": true,
    "debug": true,
    "sp": {
        "entityId": "fastapi-saml-client",
        "assertionConsumerService": {
            "url": "http://service-provider.docker.localhost:9000/saml/acs",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
        },
        "singleLogoutService": {
            "url": "http://service-provider.docker.localhost:9000/saml/sls",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
        }
    },
    "idp": {
        "entityId": "http://identity-broker.docker.localhost:9000/realms/fastapi",
        "singleSignOnService": {
            "url": "http://identity-broker.docker.localhost:9000/realms/fastapi/protocol/saml",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
        },
        "singleLogoutService": {
            "url": "http://identity-broker.docker.localhost:9000/realms/fastapi/protocol/saml",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
        },
        "x509cert": ""
    }
}